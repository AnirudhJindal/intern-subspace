{
  "name": "vscode-editor-api",
  "displayName": "VS Code Editor API",
  "description": "REST API to control VS Code editor",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onStartupFinished"],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-editor-api.start",
        "title": "Start Editor API Server"
      },
      {
        "command": "vscode-editor-api.stop",
        "title": "Stop Editor API Server"
      }
    ]
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  }
}